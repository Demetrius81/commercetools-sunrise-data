{
  "name": "commercetools-sunrise-data",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/commercetools/commercetools-sunrise-data.svg?branch=master)](https://travis-ci.org/commercetools/commercetools-sunrise-data)",
  "main": "index.js",
  "config": {
    "authUrl": "https://auth.commercetools.com",
    "apiUrl": "https://api.commercetools.com"
  },
  "scripts": {
    "set:projectKey": "npm config set commercetools-sunrise-data:project",
    "set:clientId": "npm config set commercetools-sunrise-data:client_id",
    "set:clientSecret": "npm config set commercetools-sunrise-data:client_secret",
    "get:config": "echo \"projectKey\": && npm config get commercetools-sunrise-data:project && echo \"clientId\": && npm config get commercetools-sunrise-data:client_id && echo \"clientSecret\": && npm config get commercetools-sunrise-data:client_secret",
    "build": "rimraf dist/ && babel ./**/*.js --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "credentials": "npm run build && node dist/lib/update-credentials.js --project $npm_package_config_project --client_id $npm_package_config_client_id --client_secret $npm_package_config_client_secret --apiUrl $npm_package_config_apiUrl --authUrl $npm_package_config_authUrl",
    "clean:data": "./mvnw spring-boot:run -DprojectCleanUp=true -DprojectSetUp=false -DchannelsImport=false -DcatalogImport=false -DreserveInStore=false -DordersImport=false -Dctp.projectKey=$npm_package_config_project -Dctp.clientId=$npm_package_config_client_id -Dctp.clientSecret=$npm_package_config_client_secret -Dctp.authUrl=$npm_package_config_authUrl -Dctp.apiUrl=$npm_package_config_apiUrl",
    "import:projectSetup": "./mvnw spring-boot:run -DprojectCleanUp=false -DprojectSetUp=true -DchannelsImport=true -DcatalogImport=false -DreserveInStore=false -DordersImport=false -Dctp.projectKey=$npm_package_config_project -Dctp.clientId=$npm_package_config_client_id -Dctp.clientSecret=$npm_package_config_client_secret -Dctp.authUrl=$npm_package_config_authUrl -Dctp.apiUrl=$npm_package_config_apiUrl",
    "import:catalogs": "./mvnw spring-boot:run -DprojectCleanUp=false -DprojectSetUp=false -DchannelsImport=false -DcatalogImport=true -DreserveInStore=false -DordersImport=false -Dctp.projectKey=$npm_package_config_project -Dctp.clientId=$npm_package_config_client_id -Dctp.clientSecret=$npm_package_config_client_secret -Dctp.authUrl=$npm_package_config_authUrl -Dctp.apiUrl=$npm_package_config_apiUrl",
    "import:orders": "./mvnw spring-boot:run -DprojectCleanUp=false -DprojectSetUp=false -DcatalogImport=false -DreserveInStore=false -DordersImport=true -Dctp.projectKey=$npm_package_config_project -Dctp.clientId=$npm_package_config_client_id -Dctp.clientSecret=$npm_package_config_client_secret -Dctp.authUrl=$npm_package_config_authUrl -Dctp.apiUrl=$npm_package_config_apiUrl",
    "import:zones": "npm run build && node dist/lib/zones.js --project $npm_package_config_project --client_id $npm_package_config_client_id --client_secret $npm_package_config_client_secret --apiUrl $npm_package_config_apiUrl --authUrl $npm_package_config_authUrl",
    "import:shippingmethods": "npm run build && node dist/lib/shipping-method.js --project $npm_package_config_project --client_id $npm_package_config_client_id --client_secret $npm_package_config_client_secret --apiUrl $npm_package_config_apiUrl --authUrl $npm_package_config_authUrl",
    "sync:products": "node_modules/.bin/product-csv-sync import --matchBy sku --csv data/products.csv --projectKey $npm_package_config_project --clientId $npm_package_config_client_id  --clientSecret $npm_package_config_client_secret --sphereProtocol https --sphereAuthProtocol https",
    "sync:inventory": "node_modules/.bin/stock-import --logLevel debug --logSilent true --file data/inventory.csv --projectKey $npm_package_config_project --clientId $npm_package_config_client_id --clientSecret $npm_package_config_client_secret --sphereProtocol https --sphereAuthProtocol https",
    "sync:inventory-stores": "node_modules/.bin/stock-import --logLevel debug --logSilent true --file data/inventory-stores.csv --projectKey $npm_package_config_project --clientId $npm_package_config_client_id --clientSecret $npm_package_config_client_secret --sphereProtocol https --sphereAuthProtocol https",
    "start": "npm run build && npm run credentials && npm run clean:data && npm run import:zones && npm run import:shippingmethods && npm run import:projectSetup && npm run import:catalogs && npm run sync:products",
    "states": "npm run build && node dist/lib/machine-states.js --project $npm_package_config_project --client_id $npm_package_config_client_id --client_secret $npm_package_config_client_secret --apiUrl $npm_package_config_apiUrl --authUrl $npm_package_config_authUrl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercetools/commercetools-sunrise-data.git"
  },
  "keywords": [
    "sunrise",
    "commercetools",
    "data"
  ],
  "author": "Siawash Shibani",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/commercetools/commercetools-sunrise-data/issues"
  },
  "homepage": "https://github.com/commercetools/commercetools-sunrise-data#readme",
  "devDependencies": {
    "@commercetools/api-request-builder": "3.7.0",
    "@commercetools/sdk-client": "1.5.4",
    "@commercetools/sdk-middleware-auth": "3.4.7",
    "@commercetools/sdk-middleware-http": "2.2.6",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "nconf": "0.10.0",
    "path": "0.12.7",
    "rimraf": "2.6.2",
    "sphere-node-cli": "1.0.4",
    "sphere-node-product-csv-sync": "0.20.4",
    "sphere-stock-import": "1.3.1"
  },
  "dependencies": {}
}
