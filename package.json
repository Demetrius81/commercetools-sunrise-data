{
  "name": "commercetools-sunrise-data",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/commercetools/commercetools-sunrise-data.svg?branch=master)](https://travis-ci.org/commercetools/commercetools-sunrise-data)",
  "main": "index.js",
  "credentials": {
    "project": "",
    "client_id": "",
    "client_secret": "",
    "authUrl": "https://auth.commercetools.com",
    "apiUrl": "https://api.commercetools.com"
  },
  "scripts": {
    "build": "rimraf dist/ && babel ./**/*.js --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "credentials": "npm run build && node dist/lib/update-credentials.js --project $npm_package_credentials_project --client_id $npm_package_credentials_client_id --client_secret $npm_package_credentials_client_secret --apiUrl $npm_package_credentials_apiUrl --authUrl $npm_package_credentials_authUrl",
    "dataImport": "./mvnw spring-boot:run",
    "import:zones": "npm run build && node dist/lib/zones.js --project $npm_package_credentials_project --client_id $npm_package_credentials_client_id --client_secret $npm_package_credentials_client_secret --apiUrl $npm_package_credentials_apiUrl --authUrl $npm_package_credentials_authUrl",
    "import:shippingmethods": "npm run build && node dist/lib/shipping-method.js --project $npm_package_credentials_project --client_id $npm_package_credentials_client_id --client_secret $npm_package_credentials_client_secret --apiUrl $npm_package_credentials_apiUrl --authUrl $npm_package_credentials_authUrl",
    "product:sync": "node_modules/.bin/product-csv-sync import --matchBy sku --csv data/products/products.csv --projectKey $npm_package_credentials_project --clientId $npm_package_credentials_client_id  --clientSecret $npm_package_credentials_client_secret --sphereProtocol https --sphereAuthProtocol https",
    "ordersImport": "./mvnw spring-boot:run -Drun.arguments=ordersImport",
    "inventory:sync": "node_modules/.bin/stock-import --logLevel debug --logSilent true --file data/inventory/inventory-no-stores.csv --projectKey $npm_package_credentials_project --clientId $npm_package_credentials_client_id --clientSecret $npm_package_credentials_client_secret --sphereProtocol https --sphereAuthProtocol https",
    "start": "npm run build && npm run credentials && npm run dataImport && npm run import:zones && npm run import:shippingmethods && npm run product:sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercetools/commercetools-sunrise-data.git"
  },
  "keywords": [
    "sunrise",
    "commercetools",
    "data"
  ],
  "author": "Siawash Shibani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercetools/commercetools-sunrise-data/issues"
  },
  "homepage": "https://github.com/commercetools/commercetools-sunrise-data#readme",
  "devDependencies": {
    "sphere-node-cli": "1.0.4",
    "sphere-node-product-csv-sync": "0.20.4",
    "sphere-stock-import": "1.3.1",
    "@commercetools/api-request-builder": "3.7.0",
    "@commercetools/sdk-client": "1.5.4",
    "@commercetools/sdk-middleware-auth": "3.4.7",
    "@commercetools/sdk-middleware-http": "2.2.6",
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "nconf": "0.10.0",
    "path": "0.12.7",
    "rimraf": "2.6.2"
  }
}
