{
  "name": "sunrise",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/commercetools/commercetools-sunrise-data.svg?branch=master)](https://travis-ci.org/commercetools/commercetools-sunrise-data)",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/standard --verbose | snazzy",
    "pretty": "node_modules/prettier/bin-prettier.js lib/**",
    "build": "rimraf dist/ && babel ./lib/*.js --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean:data": "npm run build && node dist/lib/data.js",
    "import:data": "npm run import:types && npm run import:channels && npm run import:taxCategories && npm run import:zones && npm run import:projectSetup && npm run import:shippingmethods && npm run import:customers && npm run import:categories && npm run import:products && npm run import:orders",
    
    "import:projectSetup": "npm run build && node dist/lib/project-setup.js --import",
    "import:categories": "npm run build && node dist/lib/categories.js --import",
    "import:orders": "npm run build && node dist/lib/orders.js --import --csv ./data/orders.csv",
    "import:channels": "npm run build && node dist/lib/channels.js --import",
    "import:zones": "npm run build && node dist/lib/zones.js --import",
    "import:taxCategories": "npm run build && node dist/lib/tax-categories.js --import",
    "import:shippingmethods": "npm run build && node dist/lib/shipping-method.js --import",
    "import:products": "npm run build && node dist/lib/products.js --importtypes && node dist/lib/products.js --import",
    "import:inventory": "npm run build && node dist/lib/inventory.js --import",
    "import:inventory-stores": "npm run build && node dist/lib/inventory.js --import",
    "import:customers": "npm run build && node dist/lib/customers.js --import",
    "import:types": "npm run build && node dist/lib/types.js --import",
    "import:states": "npm run build && node dist/lib/lineitem-states.js --import",

    "clean:categories": "npm run build && node dist/lib/categories.js --clean",
    "clean:inventory": "npm run build && node dist/lib/inventory.js --clean",
    "clean:channels": "npm run build && node dist/lib/channels.js --clean",
    "clean:orders": "npm run build && node dist/lib/orders.js --clean",
    "clean:carts": "npm run build && node dist/lib/carts.js --clean",
    "clean:states": "npm run build && node dist/lib/lineitem-states.js --clean",
    "clean:customers": "npm run build && node dist/lib/customers.js --clean",
    "clean:shippingmethods": "npm run build && node dist/lib/shipping-method.js --clean",
    "clean:zones": "npm run build && node dist/lib/zones.js --clean",
    "clean:products": "npm run build && node dist/lib/products.js --clean",
    "clean:taxCategories": "npm run build && node dist/lib/taxCategories.js --clean",
    "clean:types": "npm run build && node dist/lib/types.js --clean",

    "start": "npm run clean:data && npm run import:data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercetools/commercetools-sunrise-data.git"
  },
  "standard": {
    "ignore": [
      "src/",
      "node_modules/"
    ]
  },
  "keywords": [
    "sunrise",
    "commercetools",
    "data"
  ],
  "author": "Siawash Shibani",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/commercetools/commercetools-sunrise-data/issues"
  },
  "homepage": "https://github.com/commercetools/commercetools-sunrise-data#readme",
  "devDependencies": {
    "@commercetools/api-request-builder": "5.3.2",
    "@commercetools/sdk-client": "2.1.0",
    "@commercetools/sdk-middleware-auth": "5.1.2",
    "@commercetools/sdk-middleware-http": "5.2.1",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "braces": "^3.0.2",
    "dotenv": "^8.0.0",
    "fstream": "^1.0.12",
    "nconf": "0.10.0",
    "path": "0.12.7",
    "rimraf": "2.6.3",
    "snazzy": "8.0.0",
    "sphere-node-cli": "^3.0.0",
    "sphere-node-product-csv-sync": "2.0.0",
    "sphere-stock-import": "^2.1.0",
    "standard": "13.1.0"
  },
  "dependencies": {
    "@commercetools/sdk-auth": "^2.1.2",
    "csvtojson": "^2.0.10",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "sphere-category-sync": "^1.2.0"
  },
  "engines": {
    "node": "^10"
  }
}
